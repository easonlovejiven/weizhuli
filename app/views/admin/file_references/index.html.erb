
<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", admin_file_references_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_file_reference_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> References</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.admin_file_reference.reference_id", :default => t("activerecord.labels.reference_id", :default => "Reference_id")) %>
          </th>
                    <th>
            <%= t("activerecord.attributes.admin_file_reference.reference_type", :default => t("Reference Type", :default => "Reference type")) %>
          </th>
                    <th>
            <%= t("activerecord.attributes.admin_file_reference.upload_id", :default => t("Upload id", :default => "Upload id")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @file_references.each do |file_reference| -%>
        <tr class="<%= cycle("odd", "even") %>">
                    <td>
            <%= link_to file_reference.id, admin_file_reference_path(file_reference) %>
          </td>
          <td>
            <%= file_reference.reference_id %>
          </td>
          <td>
            <%= file_reference.reference_type%>
          </td>
          <td>
            <%= file_reference.upload_id%>
          </td>
          <td>
            <%= l file_reference.created_at%>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", admin_file_reference_path(file_reference) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_file_reference_path(file_reference) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", admin_file_reference_path(file_reference), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <%=will_paginate @uploads%>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

